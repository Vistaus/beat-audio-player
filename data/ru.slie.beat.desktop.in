[Desktop Entry]
Name=Beat audio player
Terminal=false
Type=Application
Categories=AudioVideo;Audio;Player;
StartupNotify=true
Icon=beat
Actions=Open;Add;
TryExec=beat
Exec=beat %F
MimeType=application/ogg;application/x-cue;application/x-ogg;application/xspf+xml;audio/midi;audio/mp3;audio/mpeg;audio/mpegurl;audio/ogg;audio/prs.sid;audio/x-flac;audio/x-it;audio/x-mod;audio/x-mp3;audio/x-mpeg;audio/x-mpegurl;audio/x-ms-wma;audio/x-musepack;audio/x-s3m;audio/x-scpls;audio/x-stm;audio/x-vorbis+ogg;audio/x-wav;audio/x-xm;x-content/audio-cdda;inode/directory;

[Desktop Action Open]
Name=Open in Beat
Exec=beat %F

[Desktop Action Add]
Name=Add to playlist(Beat)
Exec=beat -a %F
